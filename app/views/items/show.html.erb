<% if user_signed_in? %>
	<div class = "container">
		<div class = "col-xs-12 col-md-12 col-offset-md-3">
			<div class = "panel panel-default iDetails">
				<% @item.photos.each do |i| %>
					<div class = "thumb_image">
						<%= image_tag i.url, :class => "thumbnail_item_image" %>
					</div>
				<% end %>			
			</div>
		</div>
	</div>
<%end%>